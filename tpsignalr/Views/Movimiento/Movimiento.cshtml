@page
<style>
    #miDiv {
        position: absolute;
        z-index: 9;
        background-color: #f1f1f1;
        border: 1px solid #d3d3d3;
        text-align: center;
        height: 100px;
        width: 100px;
        border-radius: 50%;
    }

    #miDivCursor {
        padding: 10px;
        cursor: move;
        z-index: 10;
        background-color: #2196F3;
        color: #fff;
        height: 100px;
        width: 100px;
        border-radius: 50% ;

    }
</style>

<div id="miDiv">
    <div id="miDivCursor">

        Click para Mover
    </div>
</div>

<script src="~/lib/microsoft/signalr/dist/browser/signalr.min.js"></script>
<script src="~/js/jsMovimiento.js"></script>

